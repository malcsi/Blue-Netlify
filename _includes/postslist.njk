<ul style="margin: 10%; color: #57575d">
  {% for post in postslist %}
  <li>
    <h3>
      <strong>
        <a style="color: #377a85" href="{{ post.url | url }}">{{
          post.data.title
        }}</a>
      </strong>
    </h3>
    -
    <span>
      {{ post.data.description }}
    </span>
  </li>
  {% endfor %}
</ul>
